"pamixer -t"
   XF86AudioMute

"pamixer --increase 5"
   XF86AudioRaiseVolume

"pamixer --decrease 5"
   XF86AudioLowerVolume

"xbacklight -inc 1"
   XF86MonBrightnessUp

"xbacklight -dec 1"
   XF86MonBrightnessDown

"cmus-remote -u"
  XF86AudioPlay
"cmus-remote -n"
  XF86AudioNext
"cmus-remote -r"
  XF86AudioPrev

